pipeline {
    agent {
            label "build"
    }

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "maven3"
        
    }

    stages {
        stage('Prepare-Workspace') {
            
            steps {
                // Get some code from a GitHub repository
                git 'https://github.com/venkat09docs/Maven-Java-Project.git'                
            }            
        }
    }
}
